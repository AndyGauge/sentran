<p id="notice"><%= notice %></p>

<h1>Products</h1>

<table>
  <thead>
    <tr>
      <th>Model name</th>
      <th>Slug</th>
      <th>Category</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @products.each do |product| %>
      <tr>
        <td><%= product.model_name %></td>
        <td><%= product.slug %></td>
        <td><%= product.category %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

